{
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": [
    "sanity",
    "sanity/react",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "react/jsx-no-bind": "off",
    "react/forbid-prop-types": "off",
    "react/prop-types": "off",
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "^(all|part):"
        ]
      }
    ]
  }
}